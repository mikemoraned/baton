{
  "consumer": {
    "name": "Consumer"
  },
  "provider": {
    "name": "Provider"
  },
  "interactions": [
    {
      "description": "a request for playerA",
      "providerState": "playerA exists and has games",
      "request": {
        "method": "get",
        "path": "/players/playerA"
      },
      "response": {
        "status": 200,
        "headers": {
        },
        "body": {
          "id": "playerA",
          "games": [
            {
              "id": "gameA"
            }
          ]
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          },
          "$.body.id": {
            "match": "regex",
            "regex": "[a-zA-Z-]+"
          },
          "$.body.games": {
            "min": 1
          },
          "$.body.games[*].*": {
            "match": "type"
          },
          "$.body.games[*].id": {
            "match": "regex",
            "regex": "[a-zA-Z-]+"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}